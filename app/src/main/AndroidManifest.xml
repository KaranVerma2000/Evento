<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdsc_jss.evento">

    <application
        android:name=".Evento"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EventoApp">
        <activity android:name="com.gdsc_jss.evento.ui.students.StudentActivity" />
        <activity android:name="com.gdsc_jss.evento.ui.common.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.gdsc_jss.evento.ui.login.LoginActivity"/>
    </application>

</manifest>